/**
 * CSS style for carouselTicker
 **/

.carouselTicker__list{
    margin: 56px 0 30px 0;
    padding: 10px 0;
    list-style-type: none;
    overflow: hidden;
    background: #1a1a1a;
}

.carouselTicker__item{
    margin: 0px;
    float: left;
    width: 366px;
    height: 30px;
    line-height: 30px;
    text-align: center;
}

.carouselTicker__item img {
    vertical-align: top;
}